<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
<channel>
        <title>{{ 'seo.title' | trans({}, 'tutorial') }} | {{ project.name }}</title>
        <description>{{ 'seo.description' | trans({}, 'tutorial') }}</description>
        <link>{{ url('tutorial') }}</link>
        <lastBuildDate>{{ now | date('D, d M Y H:i:s') }}</lastBuildDate>
        <pubDate>{{ now | date('D, d M Y H:i:s') }}</pubDate>
        
        {% for item in contentPaginator %}
            <item>
                <title>{{ item }}</title>
                <description>{{ item.excerpt | striptags }}</description>
                <link>{{ url('tutorial_show', {'slug': item.slug }) }}</link>
                <guid>{{ item.id }}-{{ item.slug }}</guid>
                <pubDate>{{ item.publishedAt | date('D, d M Y H:i:s') }}</pubDate>
            </item>
        {% endfor %}
</channel>
</rss>