<section class="github">
    <h3>{{ 'sidebar.github.title' | trans }}</h3>
    
    <div class="content">    
        <p>{{ 'sidebar.github.text' | trans({'%name%': project.name}) }}</p>

        <p>
            <a href="{{ project.github.sources.url }}" onclick="var w=window.open(this.href);w.focus();return false;" class="btn sws html">
                <span></span> {{ 'sidebar.github.sources' | trans }}
            </a>
        </p>
    </div>
</section>